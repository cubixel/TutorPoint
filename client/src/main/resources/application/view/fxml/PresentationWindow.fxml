<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.canvas.Canvas?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.StackPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <StackPane fx:id="pane" alignment="TOP_LEFT" prefHeight="400.0" prefWidth="600.0">
         <children>
            <Canvas fx:id="canvas" height="400.0" width="600.0" />
            <AnchorPane prefHeight="200.0" prefWidth="200.0">
               <children>
                  <Button fx:id="draw1" layoutX="386.0" layoutY="326.0" mnemonicParsing="false" onAction="#drawImage1" text="Draw Image 1" />
                  <Button fx:id="remove1" layoutX="481.0" layoutY="326.0" mnemonicParsing="false" onAction="#removeImage1" text="Remove Image 1" />
                  <Button fx:id="draw2" layoutX="386.0" layoutY="361.0" mnemonicParsing="false" onAction="#drawImage2" text="Draw Image 2" />
                  <Button fx:id="remove2" layoutX="481.0" layoutY="361.0" mnemonicParsing="false" onAction="#removeImage2" text="Remove Image 2" />
               </children>
            </AnchorPane>
         </children>
      </StackPane>
   </children>
</AnchorPane>
