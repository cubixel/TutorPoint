      <StackPane fx:id="pane" alignment="TOP_LEFT" prefHeight="400.0" prefWidth="600.0">
         <children>
            <Canvas fx:id="canvas" height="400.0" width="600.0" />
            <AnchorPane prefHeight="200.0" prefWidth="200.0">
               <children>
                  <Button fx:id="button" layoutX="506.0" layoutY="361.0" mnemonicParsing="false" onAction="#drawImage" text="Draw Image" />
                  <Button fx:id="button1" layoutX="399.0" layoutY="361.0" mnemonicParsing="false" onAction="#removeImage" text="Remove Image" />
               </children>
            </AnchorPane>
         </children>
      </StackPane>
   </children>
</AnchorPane>
